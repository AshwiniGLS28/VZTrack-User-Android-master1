<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_gravity="center"
    android:gravity="center" android:layout_width="match_parent"
    android:layout_margin="5dp"
    android:layout_height="match_parent">
    <ImageView
        android:layout_width="20dp"
        android:id="@+id/tabicon"
        android:src="@drawable/nw_upload"
        android:layout_height="20dp"></ImageView>
    <vztrack.gls.com.vztrack_user.CustumView.CustomTextView
        android:layout_width="match_parent"
        android:id="@+id/tabname"
        android:padding="5dp"
        android:layout_gravity="center"
        android:gravity="center"
        android:textColor="@color/colorWhite"
        style="@style/MyTextViewStyleForSTooltip"
        android:text="tab"
        android:layout_height="wrap_content"></vztrack.gls.com.vztrack_user.CustumView.CustomTextView>

</LinearLayout>